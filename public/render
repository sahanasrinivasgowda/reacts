import react,{Component}from "react"

class body extends Components
{
    constructor()
    {
        intial state super();
        this.state={text:'hi'}
    }
}
increment=()=>{
    this.state({text:'welcome'})

}
decerement =()=>{
    this.setState({text:'thankyou'})
}
render()
return(
    <div>
    <p>{this.state.text}</p>
    <button onMouseOver={this.decrement}>Decrement</button>
    </div>
)